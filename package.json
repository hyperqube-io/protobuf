{
  "name": "@hyperqube/v3-agent-protobuf",
  "version": "2.0.10",
  "license": "MIT",
  "main": "node/index.js",
  "types": "node/index.d.ts",
  "scripts": {
    "generate": "bash -c \"yarn generate:go\"; bash -c \"yarn generate:node\"; yarn build:node",
    "build:node": "tsc",
    "build:docker:go": "docker build -t hyperqubeio/pb-go -f docker/go.Dockerfile .",
    "build:docker:node": "docker build -t hyperqubeio/pb-node -f docker/node.Dockerfile .",
    "generate:go": "docker run --rm -it -v $(pwd):/go/src/pb hyperqubeio/pb-go",
    "generate:node": "docker run --rm -it -v $(pwd):/pb -v node_modules:/pb/node_modules hyperqubeio/pb-node",
    "prepublishOnly": "yarn build:node"
  },
  "peerDependencies": {
    "@grpc/grpc-js": "^1.5.10"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.5.10",
    "@types/google-protobuf": "^3.15.5",
    "google-protobuf": "^3.20.0-rc.2"
  },
  "devDependencies": {
    "@grpc/proto-loader": "^0.6.9",
    "grpc-tools": "^1.11.2",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^4.6.3"
  }
}
